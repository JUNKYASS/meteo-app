{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "ES5",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "lib": [
      "es2016",
      "dom",
      "WebWorker"
    ]
  },
  "include": [
    "global.d.ts",
    "images.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.js",
    "./src/**/*.jsx"
  ]
}